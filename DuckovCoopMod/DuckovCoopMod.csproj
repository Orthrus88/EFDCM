    </ItemGroup>
    <ItemGroup Condition="Exists('$(MSBuildProjectDirectory)\info.ini')">
      <_ModFiles Include="$(MSBuildProjectDirectory)\info.ini" />
    <Compile Include="Main\Mod.TransportHelpers.cs" />
    </ItemGroup>
    <MakeDir Directories="$(ModOutputDir)" />
    <Copy SourceFiles="@(_ModFiles)" DestinationFolder="$(ModOutputDir)" SkipUnchangedFiles="true" />
